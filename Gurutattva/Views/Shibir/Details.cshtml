                @Model.PinCode
            </td>
        </tr>
        <tr>
            <td>Latitude</td>
            <td>
                @Model.Latitude
            </td>
        </tr>
        <tr>
            <td>Longitude</td>
            <td>
                @Model.Longitude
            </td>
        </tr>
     @*   <tr>
            <td>Country Name</td>
            <td>
                @Model.CountryName
            </td>
        </tr>*@
        <tr>
            <td>State Name</td>
            <td>
                @Model.StateName
            </td>
        </tr>
        <tr>
            <td>District Name</td>
            <td>
                @Model.DistrictName
            </td>
        </tr>
        <tr>
            <td>Taluka Name</td>
            <td>
                @Model.TalukaName
            </td>
