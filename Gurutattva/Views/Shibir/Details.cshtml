        <tr>
            <td>Sanchalak Phone 1</td>
            <td>
                @Model.SanchalakPhoneNumber1 
            </td>
        </tr>
        <tr>
            <td>Sanchalak Name 2</td>
            <td>
                @Model.SanchalakName2
            </td>
        </tr>
        <tr>
            <td>Sanchalak Email 2</td>
            <td>
                @Model.SanchalakEmail2
            </td>
        </tr>
        <tr>
            <td>Sanchalak Phone 2</td>
            <td>
                @Model.SanchalakPhoneNumber2
            </td>
        </tr>
        
    </table>
    <a asp-controller="Shibir" asp-action="Edit"
       asp-route-id="@Model.Id" class="btn btn-secondary">Edit</a>
    &nbsp<a asp-controller="Shibir" asp-action="Delete"
            asp-route-id="@Model.Id" class="btn btn-secondary">Delete</a>
    &nbsp<a asp-controller="Shibir" asp-action="Index"
            asp-route-id="@Model.Id" class="btn btn-secondary">Home</a>

</div>